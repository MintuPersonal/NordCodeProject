<% include ./../../layout/Adminheader.ejs %>
<!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
       Basic Information
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-gear"></i> Basic</a></li>
        <li><a href="#">Basic List</a></li>
        
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title"><%=content%></h3>

          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
              <i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
          </div>
        </div>
        <div class="box-body">
        
          <form action="/Admin/product/ProductEntry" method="post">
            <div class="form-horizontal">
            <p>Product</p>
            <div class=""></div>
            <hr>
            
            <table class="table">
    
                <tbody><tr>
                    <td>Name(In English)</td>
                    <td>
                        <input class="form-control text-box single-line" id="PName" name="PName" required="required" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="PName" data-valmsg-replace="true"><%=errorData.PName%></span>
                    </td>
                </tr>
                <tr>
    
                </tr><tr>
                    <td>Name(in Bangla)</td>
                    <td>
                        <input class="form-control text-box single-line" id="PName" name="PName_BN" required="required" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="PName" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Brand</td>
                    <td>
                        <input class="form-control text-box single-line" id="Brand" name="Brand" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="Brand" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Brand</td>
                    <td>
                        <input class="form-control text-box single-line" id="Brand_BN" name="Brand_BN" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="Brand_BN" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Pre. Price</td>
                    <td>
                        <input class="form-control text-box single-line" data-val="true" data-val-number="The field mrp must be a number." id="mrp" name="mrp" required="required" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="mrp" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Discount</td>
                    <td>
                        <input class="form-control text-box single-line" data-val="true" data-val-number="The field discount must be a number." id="discount" name="discount" required="required" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="discount" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Price</td>
                    <td>
                        <input class="form-control text-box single-line" data-val="true" data-val-number="The field UnitPrice must be a number." id="UnitPrice" name="UnitPrice" required="required" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="UnitPrice" data-valmsg-replace="true"><%=errorData.UnitPrice%></span>
                    </td>
                </tr>
                <tr>
                    <td>Unit Stock</td>
                    <td>
                        <input class="form-control text-box single-line" data-val="true" data-val-number="The field UnitsInStock must be a number." id="UnitsInStock" name="UnitsInStock" required="required" type="number" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="UnitsInStock" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <!-- <tr>
                    <td>Category</td>
                    <td>
                        <input class="form-control text-box single-line" id="Category" name="Category" type="text" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="Category" data-valmsg-replace="true"></span>
                    </td>
                </tr> -->
                <tr>
                    <td>Description (in Bangla)</td>
                    <td>
                        <textarea class="form-control" cols="20" id="Description_BN" name="Description_BN" rows="2" style="height:150px"></textarea>
                        <span class="field-validation-valid text-danger" data-valmsg-for="Description" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Description (in English)</td>
                    <td>
                        <textarea class="form-control" cols="20" id="Description" name="Description" rows="2" style="height:150px"></textarea>
                        <span class="field-validation-valid text-danger" data-valmsg-for="Description" data-valmsg-replace="true"></span>
                    </td>
                </tr>
    
                <tr>
                    <td>Supplier</td>
                    <td>
                        <select class="form-control" data-val="true" data-val-number="The field SID must be a number." id="SID" name="SID">
                            <option >--Select--</option>
 
                            <% for(var i=0; i< sdata.length; i++){%>
                            
                            <option value="<%= sdata[i].ID %>"><%= sdata[i].Name %></option>

                            <%}%>
                     </select>
                        <span class="field-validation-valid text-danger" data-valmsg-for="SID" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <!-- <tr>
                    <td>Add Cart?</td>
                    <td>
                        <select class="form-control" data-val="true" data-val-number="The field ROL must be a number." id="ROL" name="ROL">
                            <option value="1">Yes</option>
                            <option value="0">No</option>
                        </select>
                        <span class="field-validation-valid text-danger" data-valmsg-for="ROL" data-valmsg-replace="true"></span>
                    </td>
                </tr> -->
           <tr>
                    <td>Is Active</td>
                    <td>
                        <select class="form-control" data-val="true" data-val-number="The field status must be a number." id="status" name="status">
                            <option value="1">Yes</option>
    <option value="2">No</option>
    </select>
    
                        <span class="field-validation-valid text-danger" data-valmsg-for="status" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <tr>
                    <td>Category</td>
                    <td>
                        <select class="form-control" data-val="true" data-val-number="The field parentid must be a number." id="parentid" name="parentid">
                            <option value="">--Select--</option>
                            <% for(var i=0; i< data.length; i++){%>
                            
                                <option value="<%=data[i].id %>"><%= data[i].PName %></option>
    
                                <%}%>
                         </select>
                        <span class="field-validation-valid text-danger" data-valmsg-for="parentid" data-valmsg-replace="true"><%=errorData.parentid%></span>
                    </td>
                </tr>
            <tr>
                    <td>Rank No</td>
                    <td>
                        <input class="form-control" data-val="true" data-val-number="The field rankid must be a number." id="rankid" name="rankid" type="number" value="">
                        <span class="field-validation-valid text-danger" data-valmsg-for="rankid" data-valmsg-replace="true"></span>
                    </td>
                </tr>
                <!-- <tr>
                    <td>Image</td>
                    <td>
    
                        <input type="file" name="img">
                    </td>
                </tr>
     -->
                <tr><td></td><td><input type="submit" value="Save" class="btn btn-default"></td></tr>
            </tbody></table>
    
    
        </div>
    </form>
        </div>
        <!-- /.box-body -->
        <div class="box-footer text-center">
        
        </div>
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <% include ./../../layout/Adminfooter.ejs %>