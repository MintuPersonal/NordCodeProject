<% include ./../../layout/Adminheader.ejs %>
<!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
       Completed Order Information
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-gear"></i> Order</a></li>
        <li><a href="#">Order List</a></li>
        
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title"><%=content%></h3>

          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
              <i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
          </div>
        </div>
        <div class="box-body">
        
        
          <table id="example1" class="table table-bordered table-striped">
            <thead>
            <tr>
              <th>S/L</th>
              <th>Order No</th>
              <th>Name</th>
              <th>Mobile</th>
              <th>Net Amount</th>
              <th>Date</th>
              <th>Area</th>
              <th>
                Address
              </th>
              <th>
                Status
              </th>
              <th>Action</th>
            </tr>
            </thead>
            <tbody>
              <% if(data[0].length>0){ for(var i = 0;i < data[0].length;i++) {
              
                var cus=data[1].filter(s=>s.CustomerId=data[0][i].CustomerId);
               
               j=i+1;
                 
                %>
            <tr>
              <td>
                <%= j  %>
              </td>
              <td>
                <%= data[0][i].TONumber  %>
              </td>
              <td><%= cus[0].Name  %></td>
              <td><%= cus[0].MobileNo  %></td>
              <td><%= data[0][i].NetPrice  %></td>
              <td>
                <%= data[0][i].DeliveryTime  %>
              </td>
              <td><%= data[0][i].Area  %></td>
              <td> <%= data[0][i].Address  %></td>
              <td> <%= data[0][i].OrderStatus=="2" ? "Processing": data[0][i].OrderStatus=="3" ? "Completed" : "Canceled"  %></td>
             
              <td><a href="/Admin/order/orderdetails/<%=data[0][i].TONumber %>">Details</a></td>
            </tr>
           
            
            <% }} %>
            </tbody>
            <!-- <tfoot>
            <tr>
              <th>Rendering engine</th>
              <th>Browser</th>
              <th>Platform(s)</th>
              <th>Engine version</th>
              <th>CSS grade</th>
            </tr>
            
            </tfoot> -->
          </table>
        </div>
        <!-- /.box-body -->
        <div class="box-footer text-center">
        
        </div>
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  <% include ./../../layout/Adminfooter.ejs %>